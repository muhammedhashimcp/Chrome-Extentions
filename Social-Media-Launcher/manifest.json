{
  "manifest_version": 2,

  "name": "Hashim Launcher",
  "description": "Quick launch Hashim's social media",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "images/hashim.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
